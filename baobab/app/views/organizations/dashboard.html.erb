<h2>Hello !</h2>

<h3>My artists:</h3>

<p>
<% if user_signed_in? %>
  <% @organization.artists.each do |artist| %>
  	<li><%= link_to "#{artist.name}", artist_path(artist)  %></li>
  <% end %>
</p>

<h3>My artworks:</h3>

<p>
	<% @organization.artworks.each do |artwork| %>
		<li><%= link_to "#{artwork.title}", artwork_path(artwork.id) %>  </li>
	<% end %>
<% end %>
</p>