<h1>Inventory items</h1>

<table>
  <thead>
    <tr>
      <th>Entry date</th>
      <th>Exit date</th>
      <th>Location</th>
      <th>Artwork Title</th>
      <th>Edition</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.entry_date %></td>
        <td><%= item.exit_date %></td>
        <td><%= item.location %></td>
        <td><%= item.artwork.title %></td>
        <td><%= item.edition %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inventory item', new_organization_item_path(@organization) %>
